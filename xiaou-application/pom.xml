<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>
    <parent>
        <groupId>com.xiaou</groupId>
        <artifactId>Code-Nest</artifactId>
        <version>${revision}</version>
    </parent>

    <artifactId>xiaou-application</artifactId>
    <packaging>jar</packaging>
    <name>xiaou-application</name>

    <dependencies>
        <!-- 引入common模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-common</artifactId>
            <version>${revision}</version>
        </dependency>
        
        <!-- 引入system模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-system</artifactId>
            <version>${revision}</version>
        </dependency>
        
        <!-- 引入user模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-user</artifactId>
            <version>${revision}</version>
        </dependency>
        
        <!-- 引入monitor模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-monitor</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入interview模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-interview</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入community模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-community</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入filestorage模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-filestorage</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入notification模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-notification</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入moment模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-moment</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入sensitive模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-sensitive</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入knowledge模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-knowledge</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入version模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-version</artifactId>
            <version>${revision}</version>
        </dependency>

        <!-- 引入moyu模块 -->
        <dependency>
            <groupId>com.xiaou</groupId>
            <artifactId>xiaou-moyu</artifactId>
            <version>${revision}</version>
        </dependency>
        
        <!-- Mysql驱动包 -->
        <dependency>
            <groupId>com.mysql</groupId>
            <artifactId>mysql-connector-j</artifactId>
        </dependency>
        <!-- SpringBoot Web容器 -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-web</artifactId>
            <exclusions>
                <exclusion>
                    <artifactId>spring-boot-starter-tomcat</artifactId>
                    <groupId>org.springframework.boot</groupId>
                </exclusion>
            </exclusions>
        </dependency>
        <!-- web 容器使用 undertow 性能更强 -->
        <dependency>
            <groupId>org.springframework.boot</groupId>
            <artifactId>spring-boot-starter-undertow</artifactId>
        </dependency>

        <dependency>
            <groupId>org.projectlombok</groupId>
            <artifactId>lombok</artifactId>
        </dependency>
    </dependencies>

</project>
